{
    "version": 2,
    "builds": [
        {
            "src": "server/index.js",
            "use": "@vercel/node",
            "config": {
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/server/index.js"
        },
        {
            "src": "/sub/(.*)",
            "dest": "/server/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/dist/$1"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}